{
  "name": "dpml",
  "version": "1.0.0",
  "description": "Deepractice Prompt Markup Language",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "example:core:parsing": "node -r @swc-node/register examples/core/basic/parsing.ts",
    "example:core:processing": "node -r @swc-node/register examples/core/processing/semantic-processing.ts",
    "example:core:custom-processor": "node -r @swc-node/register examples/core/processing/custom-tag-processor.ts"
  },
  "keywords": [
    "dpml",
    "prompt",
    "llm",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.11.18",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "turbo": "2.4.4",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.0"
  }
}
