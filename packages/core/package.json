{
  "name": "@dpml/core",
  "version": "0.1.0",
  "description": "DPML核心库：包含解析器、处理器和转换器的实现",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prebuild": "pnpm lint",
    "build": "tsup",
    "lint": "eslint src --ext .ts",
    "swc": "node -r @swc-node/register",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist node_modules"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dpml",
    "prompt",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@dpml/common": "workspace:^",
    "@types/chai": "^5.2.1",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.1",
    "chai": "^5.2.0",
    "typescript": "5.8.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@types/lodash": "^4.17.16",
    "@types/xml2js": "^0.4.14",
    "fast-xml-parser": "^5.2.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.7",
    "strnum": "^2.0.5",
    "xml2js": "^0.6.2"
  }
}