{
  "name": "@dpml/cli",
  "version": "0.1.0",
  "description": "DPML Command Line Interface",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "dpml": "dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist node_modules"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dpml",
    "cli",
    "prompt",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@dpml/common": "workspace:^",
    "@types/boxen": "^3.0.1",
    "@types/chai": "^5.2.1",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.1",
    "chai": "^5.2.0",
    "tsup": "^8.0.2",
    "typescript": "5.8.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@dpml/core": "workspace:^",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1"
  }
}
